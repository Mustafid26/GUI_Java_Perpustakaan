<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0"
      xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.fxmldemo1.AnggotaController">
    <HBox prefHeight="200.0" prefWidth="200.0" spacing="30.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="20.0"/>
        </padding>
        <Label text="Data dicari"/>
        <TextField fx:id="tfsearch" prefHeight="25.0" prefWidth="662.0"/>
    </HBox>
    <TableView fx:id="tvanggota" prefHeight="400.0" prefWidth="200.0" onMouseClicked="#pilihAnggota">
        <columns>
            <TableColumn fx:id="cidanggota" prefWidth="84.0" text="ID Anggota"/>
            <TableColumn fx:id="cnama" prefWidth="270.0" text="Nama"/>
            <TableColumn fx:id="calamat" prefWidth="270.0" text="Alamat"/>
        </columns>
    </TableView>
    <Pane prefHeight="301.0" prefWidth="800.0">
        <children>
            <GridPane layoutX="20.0" layoutY="14.0" prefWidth="600.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0"
                                       minWidth="10.0" prefWidth="122.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="478.0"
                                       minWidth="10.0" prefWidth="478.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <Label text="ID Anggota"/>
                    <Label layoutX="10.0" layoutY="17.0" text="Nama" GridPane.rowIndex="1"/>
                    <Label layoutX="10.0" layoutY="47.0" text="Alamat" GridPane.rowIndex="2"/>
                    <TextField fx:id="tfidanggota" prefHeight="17.0" prefWidth="449.0" GridPane.columnIndex="1"/>
                    <TextField fx:id="tfnama" layoutX="132.0" layoutY="13.0" prefHeight="17.0" prefWidth="449.0"
                               GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    <TextField fx:id="tfalamat" layoutX="132.0" layoutY="43.0" prefHeight="17.0" prefWidth="449.0"
                               GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                </children>
            </GridPane>
        </children>
    </Pane>
    <HBox alignment="CENTER" prefHeight="160.0" prefWidth="200.0" spacing="10.0">
        <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#add" prefHeight="25.0" prefWidth="120.0"
                    text="Add"/>
            <Button fx:id="btnEdit" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#edit"
                    prefHeight="25.0" prefWidth="120.0" text="Edit"/>
            <Button fx:id="btnDelete" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#delete"
                    prefWidth="120.0" text="Delete"/>
            <Button fx:id="btnUpdate" layoutX="114.0" layoutY="10.0" mnemonicParsing="false" onAction="#update"
                    prefWidth="120.0" text="Update"/>
            <Button fx:id="btnCancel" layoutX="166.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancel"
                    prefWidth="120.0" text="Cancel"/>
        </children>
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <padding>
            <Insets left="30.0" right="40.0"/>
        </padding>
    </HBox>
</VBox>